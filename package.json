{
  "name": "simulator",
  "version": "1.0.0",
  "main": "electron.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "dist": "electron-builder --dir"
  },
  "build": {
    "appId": "com.sandfairy1219.app",
    "productName": "Srate Simulator",
    "win": {
      "target": "nsis"
    },
    "directories": {
