    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "electron.js"
    ],
    "extraResources": [
      {
        "from": "src/assets",
        "to": "assets"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5",
    "nodemon": "^3.1.4"
  }
}
